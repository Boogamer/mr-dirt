{{> header}}

{{> public_menu}}

<table class="table table-striped table-dark mt-4" style="width:90%;margin:auto;">
    <thead>
        <tr>
            <th scope="col">Command</th>
            <th scope="col">Description</th>
            <th scope="col">Usage</th>
        </tr>
    </thead>
    <tbody>
        {{#each list}}
        <tr>
            <td>{{name}}</td>
            <td>{{description}}</td>
            <td>{{format}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>